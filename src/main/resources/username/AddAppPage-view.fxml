<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<?import de.jensd.fx.glyphs.icons525.Icons525View?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="username.controller.AddAppController"
            prefHeight="800.0" prefWidth="1300.0">
    <children>
        <VBox alignment="CENTER" prefHeight="800.0" prefWidth="62.0" spacing="20.0" style="-fx-background-color: DFC6FF;" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onbuttonBacktoHome" style="-fx-background-color: NULL;">
                    <graphic>
                        <Icons525View fill="#af71ff" glyphName="HOME" size="40" />
                    </graphic>
                </Button>
                <Button mnemonicParsing="false" style="-fx-background-color: NULL;">
                    <graphic>
                        <FontAwesomeIconView fill="#af71ff" glyphName="PIE_CHART" size="40" />
                    </graphic>
                </Button>
                <Button mnemonicParsing="false" style="-fx-background-color: NULL;">
                    <graphic>
                        <FontAwesomeIconView fill="#af71ff" glyphName="LOCK" size="40" />
                    </graphic>
                </Button>
                <Button mnemonicParsing="false" style="-fx-background-color: NULL;">
                    <graphic>
                        <FontAwesomeIconView fill="#af71ff" glyphName="LIST_UL" size="40" />
                    </graphic>
                </Button>
                <Button mnemonicParsing="false" style="-fx-background-color: NULL;">
                    <graphic>
                        <FontAwesomeIconView fill="#af71ff" glyphName="COMMENTING" size="40" />
                    </graphic>
                </Button>
                <FontAwesomeIconView fill="#828282" glyphName="GEAR" size="50" translateY="190.0">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </FontAwesomeIconView>
            </children>
        </VBox>
        <VBox layoutX="62.0" prefHeight="96.0" prefWidth="1238.0" style="-fx-background-color: ffffff;" AnchorPane.leftAnchor="62.0" AnchorPane.rightAnchor="0.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Application">
                            <font>
                                <Font name="System Bold" size="24.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="10.0" top="10.0" />
                            </HBox.margin>
                        </Text>
                        <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" style="-fx-background-color: e0dee1; -fx-background-radius: 100;" translateX="850.0" translateY="25.0">
                            <graphic>
                                <FontAwesomeIconView fill="#7c7c7c" glyphName="BELL" size="20" />
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="44.0" style="-fx-background-color: dfc6ff; -fx-background-radius: 100;" translateX="850.0" translateY="25.0">
                            <graphic>
                                <Icons525View fill="#af71ff" glyphName="USER" size="25" />
                            </graphic>
                            <HBox.margin>
                                <Insets left="10.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="500.0">
                    <children>
                        <Text fill="#757575" strokeType="OUTSIDE" strokeWidth="0.0" text="4th May, 2024" translateY="-3.0">
                            <HBox.margin>
                                <Insets left="13.0" />
                            </HBox.margin>
                        </Text>
                    </children>
                </HBox>
            </children>
        </VBox>
        <AnchorPane layoutX="62.0" layoutY="96.0" prefHeight="705.0" prefWidth="1238.0" style="-fx-background-color: E9E5E5;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="62.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="96.0">
            <children>
                <VBox alignment="CENTER" layoutX="25.0" layoutY="30.0" prefHeight="643.0" prefWidth="1188.0" style="-fx-background-color: ffffff; -fx-background-radius: 15; -fx-alignment: inline-block;" AnchorPane.leftAnchor="25.0">
                    <effect>
                        <DropShadow>
                            <color>
                                <Color red="0.7764706015586853" green="0.7607843279838562" blue="0.7607843279838562" />
                            </color>
                        </DropShadow>
                    </effect>
                    <children>
                        <Label text="App Name:" style="-fx-font-size: 17px;">
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </Label>
                        <ComboBox fx:id="appNameComboBox" prefWidth="300.0" style="-fx-background-color: #dfc6ff; -fx-border-radius: 5;">
                            <VBox.margin>
                                <Insets bottom="20.0" />
                            </VBox.margin>
                        </ComboBox>
                        <TextField fx:id="appNameTextField" prefWidth="300.0" style="-fx-background-color: #dfc6ff; -fx-border-radius: 5;">
                            <VBox.margin>
                                <Insets bottom="20.0" />
                            </VBox.margin>
                        </TextField>

                        <Label text="Weekly Limit:" style="-fx-font-size: 17px;">
                            <VBox.margin>
                                <Insets bottom="10.0" />
                            </VBox.margin>
                        </Label>
                        <Spinner fx:id="weeklyLimitSpinner" editable="true" min="0" max="168" initialValue="0" prefWidth="150.0" style="-fx-background-color: #dfc6ff; -fx-border-radius: 5;">
                            <VBox.margin>
                                <Insets bottom="20.0" />
                            </VBox.margin>
                        </Spinner>

                        <Label text="Monthly Limit:" style="-fx-font-size: 17px;">
                            <VBox.margin>
                                <Insets bottom="10.0" />
                            </VBox.margin>
                        </Label>
                        <Spinner fx:id="monthlyLimitSpinner" editable="true" min="0" max="720" initialValue="0" prefWidth="150.0" style="-fx-background-color: #dfc6ff; -fx-border-radius: 5;">
                            <VBox.margin>
                                <Insets bottom="20.0" />
                            </VBox.margin>
                        </Spinner>

                        <Button text="Save to User Preferences" onAction="#onSaveButton" style="-fx-background-color: DFC6FF; -fx-background-radius: 15; -fx-text-fill: #a259ff;" prefWidth="200.0">
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </Button>

                        <Button text="Back to User Preferences" onAction="#onBackButton" style="-fx-background-color: #E9E5E5; -fx-background-radius: 15; -fx-text-fill: #868686;" prefWidth="200.0">
                            <VBox.margin>
                                <Insets top="10.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
